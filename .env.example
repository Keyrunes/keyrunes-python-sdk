# Keyrunes SDK - Environment Variables
# Copy this file to .env and adjust values for your environment

# Keyrunes API Configuration
KEYRUNES_BASE_URL=http://localhost:8080
KEYRUNES_API_KEY=

# Database (if running separately)
DATABASE_URL=postgres://keyrunes:keyrunes_dev_password@localhost:5432/keyrunes

# JWT Configuration
JWT_SECRET=dev_jwt_secret_change_in_production
JWT_EXPIRATION=3600

# Admin Configuration
ADMIN_KEY=dev_admin_key_change_in_production

# Test User Credentials (for local testing)
TEST_USER_USERNAME=testuser
TEST_USER_EMAIL=testuser@example.com
TEST_USER_PASSWORD=testpass123

# Test Admin Credentials (for local testing)
TEST_ADMIN_USERNAME=adminuser
TEST_ADMIN_EMAIL=admin@example.com
TEST_ADMIN_PASSWORD=adminpass123

# Logging
LOG_LEVEL=INFO
